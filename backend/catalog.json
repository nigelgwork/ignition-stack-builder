      "id": "ignition",
      "name": "Ignition",
      "category": "Industrial Platforms",
      "description": "Industrial automation platform with SCADA, MES, and IIoT capabilities",
      "image": "inductiveautomation/ignition",
      "default_version": "latest",
      "available_versions": ["latest", "8.3.2", "8.3.1", "8.3.0", "8.1.45", "8.1.44", "8.1.43", "8.1.42", "8.1.41", "8.1.40", "8.1.39", "8.1.38", "8.1.37", "8.1.36", "8.1.35", "8.1.34", "8.1.33", "8.1.32"],
      "supports_multiple": true,
      "default_config": {
        "ports": ["8088:8088", "8043:8043"],
        "environment": {
          "ACCEPT_IGNITION_EULA": "Y",
          "GATEWAY_ADMIN_USERNAME": "admin",
          "GATEWAY_ADMIN_PASSWORD": "password",
          "IGNITION_EDITION": "standard",
          "IGNITION_MEMORY_MAX": "2048m",
          "IGNITION_MEMORY_INIT": "512m"
        },
        "volumes": ["./configs/{instance_name}/data:/usr/local/bin/ignition/data"]
      },
      "configurable_options": {
        "name": {"type": "text", "default": "ignition-1", "label": "Instance Name"},
        "version": {"type": "select", "default": "latest", "label": "Version"},
        "http_port": {"type": "number", "default": 8088, "label": "HTTP Port"},
        "https_port": {"type": "number", "default": 8043, "label": "HTTPS Port"},
        "admin_username": {"type": "text", "default": "admin", "label": "Admin Username"},
        "admin_password": {"type": "password", "default": "password", "label": "Admin Password"},
        "edition": {"type": "select", "default": "standard", "label": "Edition", "options": ["standard", "edge", "maker"]},
        "modules": {
          "type": "multiselect",
          "default": ["perspective", "vision", "tag-historian", "sql-bridge", "alarm-notification", "opc-ua", "reporting"],
          "label": "Modules",
          "options": [
            "perspective",
            "vision",
            "tag-historian",
            "sql-bridge",
            "alarm-notification",
            "opc-ua",
            "sfc",
            "reporting",
            "symbol-factory",
            "enterprise-administration",
            "sms-notification",
            "voice-notification"
          ]
        },
        "third_party_modules": {"type": "textarea", "default": "", "label": "3rd Party Module URLs (one per line)", "placeholder": "https://example.com/module.modl"},
        "commissioning_allow_non_secure": {"type": "checkbox", "default": true, "label": "Allow Non-Secure Commissioning"},
        "quick_start": {"type": "checkbox", "default": true, "label": "Enable Quick Start"},
        "memory_max": {"type": "text", "default": "2048m", "label": "JVM Max Memory (e.g., 2048m, 4g)"},
        "memory_init": {"type": "text", "default": "512m", "label": "JVM Initial Memory (e.g., 512m, 1g)"}
      },
      "integrations": ["db_client", "mqtt_client", "oauth_client"],
      "enabled": true
    },
